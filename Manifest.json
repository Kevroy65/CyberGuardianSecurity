{
    "manifest_version": 3,
    "name": "Cyber Guardian Security",
    "description": "A security based software",
    "version": "1.0.0",
    "icons": {
      "128": "App-logo_128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "App-logo_19.png"
      }
    },
    "permissions": [
      "alarms",
      "contextMenus",
      "system.cpu",
      "system.memory",
      "power",
      "desktopCapture",
      "geolocation",
      "system.display",
      "system.storage",
      "storage",
      "webRequest"
    ]
}
  